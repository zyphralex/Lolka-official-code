#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules/node-gyp/bin/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules/node-gyp/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/node_modules:/mnt/c/Users/Daniil/Desktop/node_modules:/mnt/c/Users/Daniil/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules/node-gyp/bin/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules/node-gyp/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node-gyp@10.3.1/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules/.pnpm/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/key-listener/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/lib/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/node_modules:/mnt/c/Users/Daniil/Desktop/node_modules:/mnt/c/Users/Daniil/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Daniil/Desktop/electron-private/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
fi
